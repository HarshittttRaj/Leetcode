   bool isPalindrome(int x) {
         if(x<0){
            return false;
        }
       long int reverse=0, rem=0,n=x; 
        
       
  while(x!=0)    
  {    
     rem=x%10;      
     reverse=reverse*10+rem;    
     x/=10;    
  }    
        if(reverse==n){
            return true;
        }
        else{
        return false;
        }
    }
